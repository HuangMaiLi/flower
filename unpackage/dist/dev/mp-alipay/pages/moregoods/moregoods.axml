<view class="home"><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{5000}}" circular="{{true}}"><swiper-item a:for="{{swipers}}" a:for-item="item" a:for-index="index" a:key="link_id"><image src="{{item.image_url}}" data-event-opts="{{[['tap',[['swipersClick',[index]]]]]}}" onTap="__e"></image></swiper-item></swiper><image src="{{img1}}" mode="widthFix"></image><view data-event-opts="{{[['tap',[['list1Click',[index]]]]]}}" class="l1" onTap="__e" a:for="{{list1}}" a:for-item="item" a:for-index="index" a:key="link_id"><image src="{{item.image_url}}" mode="widthFix"></image></view><view data-event-opts="{{[['tap',[['list2Click',[index]]]]]}}" class="l2" onTap="__e" a:for="{{list2}}" a:for-item="item" a:for-index="index" a:key="link_id"><image src="{{item.image_url}}" mode="widthFix"></image></view><image src="{{img2}}" mode="widthFix"></image><view class="goods_list"><view data-event-opts="{{[['tap',[['itemClick',['$0'],[[['arr','id',item.id]]]]]]]}}" class="goods_item" onTap="__e" a:for="{{arr}}" a:for-item="item" a:for-index="__i0__" a:key="id"><image src="{{item.image_url}}" mode="widthFix"></image><view class="tit"><text>{{item.title}}</text></view><view class="sub_title"><text>{{item.sub_title}}</text></view><view class="price"><text>{{"¥"+item.price}}</text></view></view></view><button data-event-opts="{{[['tap',[['goMore',['$event']]]]]}}" class="more" onTap="__e">查看更多</button></view>