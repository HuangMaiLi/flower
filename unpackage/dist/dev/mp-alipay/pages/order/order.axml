<view class="content"><view class="navbar"><view data-event-opts="{{[['tap',[['tabClick',[index]]]]]}}" class="{{(('nav-item')+' '+((tabCurrentIndex===index)?'current':''))}}" onTap="__e" a:for="{{navList}}" a:for-item="item" a:for-index="index" a:key="index">{{''+item.text+''}}</view></view><swiper class="swiper-box" current="{{tabCurrentIndex}}" duration="300" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" onChange="__e"><swiper-item class="tab-content" a:for="{{navList}}" a:for-item="tabItem" a:for-index="tabIndex" a:key="tabIndex"><scroll-view class="list-scroll-content" scroll-y="{{true}}" data-event-opts="{{[['scrollToLower',[['loadData',['$event']]]]]}}" onScrollToLower="__e"><block a:if="{{tabItem.loaded===true&&tabItem.orderList.length===0}}"><empty vue-id="{{'3c46aa78-1-'+tabIndex}}" onVueInit="__l"></empty></block><view class="order-item" a:for="{{tabItem.orderList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="i-top b-b"><text class="time">{{item.time}}</text><text class="state" style="{{'color:'+(item.stateTipColor)+';'}}">{{item.stateTip}}</text><block a:if="{{item.state===9}}"><text data-event-opts="{{[['tap',[['deleteOrder',[index]]]]]}}" class="del-btn yticon icon-iconfontshanchu1" onTap="__e"></text></block></view><block a:if="{{item.goodsList.length>1}}"><scroll-view class="goods-box" scroll-x="{{true}}"><view class="goods-item" a:for="{{item.goodsList}}" a:for-item="goodsItem" a:for-index="goodsIndex" a:key="goodsIndex"><image class="goods-img" src="{{goodsItem.image}}" mode="aspectFill"></image></view></scroll-view></block><block a:for="{{item.goodsList}}" a:for-item="goodsItem" a:for-index="goodsIndex" a:key="goodsIndex"><block a:if="{{item.goodsList.length===1}}"><view class="goods-box-single"><image class="goods-img" src="{{goodsItem.image}}" mode="aspectFill"></image><view class="right"><text class="title clamp">{{goodsItem.title}}</text><text class="attr-box">{{goodsItem.attr+"  x "+goodsItem.number}}</text><text class="price">{{goodsItem.price}}</text></view></view></block></block><view class="price-box">共<text class="num">7</text>件商品 实付款<text class="price">143.7</text></view><block a:if="{{item.state!=9}}"><view class="action-box b-t"><button data-event-opts="{{[['tap',[['cancelOrder',['$0'],[[['navList','',tabIndex],['orderList','',index]]]]]]]}}" class="action-btn" onTap="__e">取消订单</button><button class="action-btn recom">立即支付</button></view></block></view><uni-load-more vue-id="{{'3c46aa78-2-'+tabIndex}}" status="{{tabItem.loadingType}}" onVueInit="__l"></uni-load-more></scroll-view></swiper-item></swiper></view>