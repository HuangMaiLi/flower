<view class="{{(('popup-layout-wrap ')+' '+popuplayoutClass)}}"><view class="{{(('popup-layout-content')+' '+popupContentClass)}}" style="{{'height:'+(height)+';'}}"><view class="link-address-wrap"><view class="link-adress-content"><view class="head-wrap"><text data-event-opts="{{[['tap',[['btn_cancel',['$event']]]]]}}" class="cancel" onTap="__e">取消</text><text data-event-opts="{{[['tap',[['btn_confirm',['$event']]]]]}}" class="confirm" onTap="__e">确认</text></view><view class="head-selected"><text class="selected-txt">{{"已选择:"+selected_address}}</text></view><view class="operation-wrap"><view class="operation-container"><view class="operation-content" a:for="{{linkAddress_area}}" a:for-item="base_items" a:for-index="base_index"><scroll-view class="province" scroll-y="true" show-scrollbar="false"><view value="{{items.id}}" data-event-opts="{{[['tap',[['choosefun',['$0','$1'],[[['linkAddress_area','',base_index,'clickfun']],[['linkAddress_area','',base_index],['info','',index,'id']]]]]],['input',[['__set_model',['$0','id','$event',[]],[[['linkAddress_area','',base_index],['info','',index]]]]]]]}}" class="{{(index==base_items.current?'province-txt-click':'province-txt')}}" onTap="__e" onInput="__e" a:for="{{base_items.info}}" a:for-item="items" a:for-index="index">{{''+items.name+''}}<view hidden="{{!(index===base_items.current)}}" class="pic"><image src="../../static/xuan-linkAddress/yes.png"></image></view></view></scroll-view></view></view></view></view></view></view><block a:if="{{maskShow}}"><view data-event-opts="{{[['tap',[['close',['$0'],['maskClick']]]]]}}" class="popup-layout-mask" onTap="__e"></view></block></view>