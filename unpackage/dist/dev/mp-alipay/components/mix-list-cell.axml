<view class="content"><view class="{{(('mix-list-cell')+' '+border)}}" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['eventClick',['$event']]]]]}}" onTap="__e"><block a:if="{{icon}}"><text class="{{(('cell-icon yticon')+' '+icon)}}" style="{{'color:'+(iconColor)+';'}}"></text></block><text class="cell-tit clamp">{{title}}</text><block a:if="{{tips}}"><text class="cell-tip">{{tips}}</text></block><text class="{{(('cell-more yticon')+' '+typeList[navigateType])}}"></text></view></view>